     var clicker = "recaptcha-consent-button";
     function plural(num, txt) {
         if (num == 0) return "";
         if (num == 1) return num + " " + txt;
         return num + " " + txt + "s";
     };
     var millis = new Date() - new Date("2020-10-25 10:42");
     var quotient = 7 * 24 * 60 * 60 * 1000;
     var weeks = Math.floor(millis / quotient);
     millis = millis % quotient;
     quotient /= 7;
     var days = Math.floor(millis / quotient);
     millis = millis % quotient;
     quotient /= 24;
     var hours = Math.floor(millis / quotient);
     millis = millis % quotient;
     quotient /= 60;
     var minutes = Math.floor(millis / quotient);
     var durations = [
         plural(weeks, "week"),
         plural(days, "day"),
         plural(hours, "hour"),
         plural(minutes, "minute"),
     ];
     var duration = durations
         .filter(function(x){return x!==""})
         .join(", ")
         .replace(/^(.*[^,], )/, "$1and ")
         .replace(/^([^,]+), and/, "$1 and")
     ;
     setters["description"] = setters["description"].replace(/%%/, duration);
     for(var key in setters) {
         document.getElementById(key).value = setters[key];
      }
      document.getElementById(clicker).click(); })()